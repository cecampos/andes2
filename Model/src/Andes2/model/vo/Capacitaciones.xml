<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="Capacitaciones"
  Version="11.1.2.62.76"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full"
  UseGlueCode="false">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="Andes2.model.vo.Capacitaciones_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="elCargo"
    Kind="where"
    Type="java.lang.String"/>
  <SQLQuery><![CDATA[SELECT capacitaciones.EMPL_RUT rut, extract(day from CAPT_FECHA_INICIO) dia, turnos.turn_tipo turno
FROM capacitaciones, turnos,contrato_empleados
WHERE cast(substr(capacitaciones.capt_hora_inicio,0,2) AS int) >= cast(substr(turnos.turn_hora_ingreso,0,2) as int)
AND cast(substr(capacitaciones.capt_hora_termino,0,2) AS int) <= cast(substr(turnos.turn_hora_salida,0,2) as int)
AND contrato_empleados.empl_rut = capacitaciones.empl_rut and contrato_empleados.crgo_id = :elCargo]]></SQLQuery>
  <ViewAttribute
    Name="Rut"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="255"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="RUT"
    Expression="RUT"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="20"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="Dia"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.math.BigDecimal"
    ColumnType="NUMBER"
    AliasName="DIA"
    Expression="DIA"
    SQLType="NUMERIC"/>
  <ViewAttribute
    Name="Turno"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="4"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="TURNO"
    Expression="TURNO"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="4"/>
    </DesignTime>
  </ViewAttribute>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="Andes2.model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
