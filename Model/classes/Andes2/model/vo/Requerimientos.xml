<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="Requerimientos"
  Version="11.1.2.62.76"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full"
  UseGlueCode="false">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="Andes2.model.vo.Requerimientos_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="elCargo"
    Kind="where"
    Type="java.lang.String"/>
  <SQLQuery><![CDATA[SELECT extract(day from capt_fecha) dia,
       turn_nombre turno,
       capt_requerimiento demanda
FROM capacity_turno
where crgo_id = :elCargo]]></SQLQuery>
  <ViewAttribute
    Name="Dia"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.math.BigDecimal"
    ColumnType="NUMBER"
    AliasName="DIA"
    Expression="DIA"
    SQLType="NUMERIC"/>
  <ViewAttribute
    Name="Turno"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="20"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="TURNO"
    Expression="TURNO"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="20"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="Demanda"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="38"
    Scale="0"
    Type="java.math.BigInteger"
    ColumnType="NUMBER"
    AliasName="DEMANDA"
    Expression="DEMANDA"
    SQLType="NUMERIC"/>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="Andes2.model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
