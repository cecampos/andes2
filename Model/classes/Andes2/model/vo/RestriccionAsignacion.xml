    PrecisionRule="true"
    Type="java.math.BigDecimal"
    ColumnType="NUMBER"
    AliasName="DIA"
    Expression="DIA"
    SQLType="NUMERIC"/>
  <ViewAttribute
    Name="Turno"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="20"
