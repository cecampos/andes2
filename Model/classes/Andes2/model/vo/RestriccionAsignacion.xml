  <SQLQuery><![CDATA[SELECT RESTRICCION_ASIGNACION.EMPL_RUT,
  extract(day from RESTRICCION_ASIGNACION.RTAG_INICIO) dia_inicio,
  extract(month from RESTRICCION_ASIGNACION.RTAG_INICIO) mes_inicio,
  extract(year from RESTRICCION_ASIGNACION.RTAG_INICIO) annio_inicio,
  extract(day from RESTRICCION_ASIGNACION.RTAG_TERMINO) dia_termino,
  extract(month from RESTRICCION_ASIGNACION.RTAG_TERMINO) mes_termino,
  extract(year from RESTRICCION_ASIGNACION.RTAG_TERMINO) annio_termino,
  extract(day from (last_day(RESTRICCION_ASIGNACION.RTAG_MES))) dia_mes,
  extract(month from RESTRICCION_ASIGNACION.RTAG_MES) mes_mes,
  extract(year from RESTRICCION_ASIGNACION.RTAG_MES) annio_mes,
   RESTRICCION_ASIGNACION.RTAG_TODOS,
  RESTRICCION_ASIGNACION.RTAG_LUNES,
  RESTRICCION_ASIGNACION.RTAG_MARTES,
  RESTRICCION_ASIGNACION.RTAG_MIERCOLES,
  RESTRICCION_ASIGNACION.RTAG_JUEVES,
  RESTRICCION_ASIGNACION.RTAG_VIERNES,
  RESTRICCION_ASIGNACION.RTAG_SABADO,
  RESTRICCION_ASIGNACION.RTAG_DOMINGO,
  RESTRICCION_ASIGNACION.RTAG_APERTURA,
  RESTRICCION_ASIGNACION.RTAG_TARDE,
  RESTRICCION_ASIGNACION.RTAG_NOCHE
FROM RESTRICCION_ASIGNACION
INNER JOIN CONTRATO_EMPLEADOS ON CONTRATO_EMPLEADOS.EMPL_RUT=RESTRICCION_ASIGNACION.EMPL_RUT
WHERE CONTRATO_EMPLEADOS.crgo_id=:elCargo OR RESTRICCION_ASIGNACION.EMPL_RUT=RESTRICCION_ASIGNACION.EMPL_RUT]]></SQLQuery>
